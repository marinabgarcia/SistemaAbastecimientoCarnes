	<parameter name="totala" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     sum( venta.`precioTotalVenta`) AS venta_sumaP,
     sum( compra.`precioTotalCompra`) AS compra_suma,
     $P{totala} AS totala
FROM
     `venta` venta,
     `compra` compra

WHERE
 compra.`estadoCompra`='Pagada' &&
 venta.`estadoVenta`='Pagado'&&
 venta.`fechaVenta`<= $P{fecha_fina}&&
 venta.`fechaVenta`>= $P{fecha_inia} &&
 compra.`fechaCompra`<=$P{fecha_fina} &&
 compra.`fechaCompra`>=$P{fecha_inia}]]>
	</queryString>
	<field name="venta_sumaP" class="java.lang.Double"/>
	<field name="compra_suma" class="java.lang.Double"/>
	<field name="totala" class="java.lang.Object"/>
	<variable name="TotalActivo" class="java.lang.Double">
		<variableExpression><![CDATA[new java.lang.Double ($F{venta_sumaP})-new java.lang.Double($F{compra_suma})]]></variableExpression>
